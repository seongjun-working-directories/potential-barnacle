  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "editor.snippetSuggestions": "inline",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.validate": [
    "vue",
    "javascript",
    "typescript"
  ],
  "prettier.semi": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true
  },
